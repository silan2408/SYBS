
@{
    ViewBag.Title = "KurumEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <form action="/KurumTanim/KurumEkle2/">
      
        <div class="form-group">
            <label>Firma Adı</label>
            <input type="text" name="FIRMA_ADI" class="form-control" />

        </div>
        <div class="form-group">
            <label>Firma Yetkilisi</label>
            <input type="text" name="FIRMA_YEKTILISI" class="form-control" />

        </div>
        <div class="form-group">
            <label>Firma Adresi</label>
            <input type="text" name="FIRMA_ADRESİ" class="form-control" />

        </div>
        <div class="form-group">
            <label>Telefon</label>
            <input type="tel" name="TELEFON" class="form-control" />

        </div>
        <div class="form-group">
            <label>Email</label>
            <input type="email" name="EMAIL" class="form-control" />

        </div>
        <div class="form-group">
            <button class="btn btn-success">Kurum Ekle</button>
        </div>
    </form>
</div>
